.owl-carousel .owl-wrapper{
    margin-left: -10px;
    margin-right: -10px;
}

.craftsmen .inner-wrapper{
    position: relative;
}

.owl-carousel .item{
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 20px;
}
.owl-carousel .card {
    background-color: #ffffff;
    border: solid 1px #d1d1d1;
    text-align: center;
}
.craftsmen .card-description {
    padding: 35px 37px;
}
.owl-carousel .card-description img{
    width:auto !important;
    margin-left: auto !important;
    margin-right: auto !important;
}
.craftsmen .card-image img {
    width: 100%;
}
.owl-wrapper-outer{
    overflow: hidden !important; 
}

.owl-buttons {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 100%;
}
.owl-buttons .owl-prev, .owl-buttons .owl-next {
    position: absolute;
    z-index: 10;
}
.owl-buttons .owl-prev{ left: -65px;}
.owl-buttons .owl-next{ right: -65px;}


.owl-buttons .fa {
    width: 40px;
    height: 40px;
    border: solid 1px #5c5c68;
    border-radius: 50%;
    line-height: 40px;
    font-size: 22px;
    text-align: center;
    margin-left: 10px;
    margin-right: 10px;
}

@media screen and (max-width:992px){
    .owl-buttons .owl-prev{ left: 0px;}
    .owl-buttons .owl-next{ right: 0px;}
    
    .owl-buttons {
        position: static;
        transform: none;
        width: 100%;
        text-align: center;
        padding-top: 15px;
        padding-bottom: 15px;
    }

    
    .owl-buttons .owl-prev, .owl-buttons .owl-next {
        position: static;
        display: inline-block;

    }
    

}
@media screen and (max-width:767px){
    .craftsmen.owl-carousel .owl-wrapper{
        margin-left: 0px;
        margin-right: 0px;
    }



}


.owl-carousel .card:hover {
