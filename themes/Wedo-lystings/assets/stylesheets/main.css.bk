    font-size: 24px;
    font-weight: 600;
    color: #000000;
    font-family: 'Open Sans', sans-serif;
    display: inline-block;
    vertical-align: middle;
    margin-bottom: 0;
}
.box p{
    font-size: 16px;
    color: #7e7e89;
    font-family: 'Open Sans', sans-serif;
    line-height: 29px;
    margin-bottom: 20px;
}
.box hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 2px solid #d1d1d1;
    margin: 35px 0 35px 0 !important;
    padding: 0;
}
.box .list1{
    list-style-type: none;
    margin-bottom: 15px;
}
.box .list1 li{
    font-family: 'Open Sans', sans-serif;
    font-size: 13px;
    display: inline-block;
    font-weight: 600;
    margin-bottom: 15px;
}
.box .list1 li:after{
   content: "|";
    font-weight: 700;
    margin: 0px 12px;
}
.box .list1 li:last-child:after{
    display: none;
}
.box .list1 li a{color: #000000;}
.box .gallery{list-style-type: none;display: inline-block;    margin-right: 38px;}
.box .gallery li img{width:226px; height:162px; object-fit: cover;object-position: center center;}
.box .view-btn{ display: inline-block;
    vertical-align: middle;
    font-family: 'Open Sans', sans-serif;
    font-size: 16px;
    font-weight: 600;
    text-align: center;
    color: #58595b;
}
.box .view-btn .fa{
    width: 40px;
    height: 40px;
    border: solid 1px #5c5c68;
    display: block;
    border-radius: 50%;
    line-height: 40px;
    font-size: 22px;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 10px;
    
}
.box .gallery li{display: inline-block;margin-right: 17px;}
.list2{margin-top: 13px;margin-bottom: 20px;}
.list2 li{margin-bottom: 13px;}
.list2 li , .list2 li a , .list2 li i , .list2 li address{font-family: 'Open Sans', sans-serif;font-size: 16px;color: #5c5c68;word-break: break-all;vertical-align: middle;}
.list2 li address{margin-bottom: 0px;padding-left: 29px;}
.list2 li i{margin-right: 13px;float: left;width: 16px;}
.timings li{font-family: 'Open Sans', sans-serif;font-size: 13px;line-height: 2.15;color: #5c5c68;}
.box h5{font-family: 'Open Sans', sans-serif;font-size: 16px;font-weight: 600;color: #ffa602;margin-bottom: 6px;}


@media screen and (max-width:992px){
